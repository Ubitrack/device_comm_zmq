                <Predicate>type=='Intrinsics'&amp;&amp;mode=='pull'</Predicate>

            </Edge>
        </Input>

        <Output>
            <Node name="ZMQSocket" displayName="ZMQ Socket Module configuration">
                <Attribute name="socketUrl" displayName="Socket URL" default="tcp://0.0.0.0:9977" xsi:type="StringAttributeDeclarationType"/>
                <Attribute name="bindTo" xsi:type="EnumAttributeReferenceType"/>
                <Attribute name="fixTimestamp" xsi:type="EnumAttributeReferenceType"/>
                <Attribute name="verbose" xsi:type="EnumAttributeReferenceType"/>
                <Attribute name="serialization_method" xsi:type="EnumAttributeReferenceType"/>
            </Node>
        </Output>

        <DataflowConfiguration>
            <UbitrackLib class="ZMQPullSinkCameraIntrinsics"/>
        </DataflowConfiguration>
    </Pattern>
	
    
    <Pattern name="ZMQPullSinkImage" displayName="ZMQ PullSink (Image)">
        <Description><h:p>This pattern transmits a Image via the selected transport to a ZMQPullSourceImage having the same ID.</h:p></Description>
        
        <Input>
            <Node name="A" displayName="A"/>
            <Node name="B" displayName="B"/>
            <Edge name="Input" source="A" destination="B" displayName="Input">
                <Description><h:p>The Image received via zmq</h:p></Description>
                <Predicate>type=='Image'&amp;&amp;mode=='pull'</Predicate>

            </Edge>
        </Input>

        <Output>
            <Node name="ZMQSocket" displayName="ZMQ Socket Module configuration">
                <Attribute name="socketUrl" displayName="Socket URL" default="tcp://0.0.0.0:9977" xsi:type="StringAttributeDeclarationType"/>
                <Attribute name="bindTo" xsi:type="EnumAttributeReferenceType"/>
                <Attribute name="fixTimestamp" xsi:type="EnumAttributeReferenceType"/>
                <Attribute name="verbose" xsi:type="EnumAttributeReferenceType"/>
                <Attribute name="serialization_method" xsi:type="EnumAttributeReferenceType"/>
            </Node>
        </Output>

        <DataflowConfiguration>
            <UbitrackLib class="ZMQPullSinkImage"/>
        </DataflowConfiguration>
    </Pattern>
   

